{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "scaleSetNamePrefix": {
      "value": "tpss"
    },
    "vmPrefix": {
      "value": "vm"
    },
    "vmSku": {
      "value": "Standard_D4ds_v4"
    },
    "adminUsername": {
      "value": "azureuser"
    },
    "scaleSetDefaultVMCount": {
      "value": 2
    },
    "scaleSetMaxVMCount": {
      "value": 100
    },
    "scaleSetScaleByCount": {
      "value": 2
    },
    "scaleSetScaleOutWhenCpuAbove": {
      "value": 50
    },
    "scaleSetScaleInWhenCpuBelow": {
      "value": 30
    },
    "vmCpuAlertThreshold": {
      "value": 75
    },
    "vmFreeMemoryGBLessThanAlertThreshold": {
      "value": 10
    }
  }
}